{
  "version": "1.0",
  "project": {
    "name": "GAN-Inspired Validation Rules",
    "version": "1.0.0",
    "description": "Dual validation system inspired by GAN architecture"
  },
  "ai_behavior": {
    "roles": {
      "developer1": {
        "type": "generator",
        "responsibilities": [
          "analyze_requirements",
          "propose_solutions",
          "implement_changes",
          "document_decisions"
        ],
        "validation_steps": [
          "verify_existing_files",
          "check_dependencies",
          "plan_implementation",
          "separate_ui_logic",
          "verify_context"
        ]
      },
      "developer2": {
        "type": "discriminator",
        "responsibilities": [
          "validate_proposals",
          "challenge_assumptions",
          "identify_issues",
          "ensure_quality"
        ],
        "validation_criteria": [
          "file_structure",
          "dependencies",
          "code_principles",
          "security",
          "error_handling"
        ]
      }
    },
    "workflow": {
      "require_dual_validation": true,
      "validation_steps": [
        "dev1_proposal",
        "dev2_review",
        "clarification_if_needed",
        "implementation_approval"
      ]
    }
  }
}